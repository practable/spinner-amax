gCAA4E;AAA1C,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,QAAA,CAAA,gBAAA,CAAgB,CAAA,IAAhB,CAAF;AAAA,KAAA;AAAqC,GAAxG,EAA8F,GAA9F,CAA8G,CAAjI,CAGE,EAFF,WAEE,EADF,mBAAA,CAAiI,KAAjI,EAAA,WAAA,EAAiI,CAA9G,mBAAA,CAAwG,QAAxG,EAAwG;AAAhG,IAAA,IAAI,EAAC,QAA2F;AAAlF,IAAA,KAAK,EAAC,gCAA4E;AAA1C,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA,UAAA,MAAA;AAAA,aAAE,QAAA,CAAA,gBAAA,CAAgB,CAAA,IAAhB,CAAF;AAAA,KAAA;AAAqC,GAAxG,EAA8F,GAA9F,CAA8G,CAAjI,CACE,CAJN,C,CAPJ,E;;AAAA,GAsBE,EARF,WAQE,EAPoC,KAAA,CAAA,gB,kBAAtC,mBAAA,CAEM,KAFN,EAAA,WAAA,EAEM,CADF,YAAA,CAAuJ,0BAAvJ,EAAuJ;AAArI,IAAA,WAAW,EAAE,IAAA,CAAA,yBAAwH;AAA5F,IAAA,QAAQ,EAAE,CAAkF;AAA9E,IAAA,QAAQ,EAAE,GAAoE;AAA9D,IAAA,aAAa,EAAE,EAA+C;AAA1C,IAAA,kBAAkB,EAAE;AAAsB,GAAvJ,E,IAAA,E;;AAAA,I,eAAA,CACE,CAFN,C,mBAGA,mBAAA,CAEM,KAFN,EAAA,WAAA,EAEM,CADF,mBAAA,CAAsF,OAAtF,EAAsF;AAA/E,IAAA,IAAI,EAAC,MAA0E;AAAnE,IAAA,KAAK,EAAC,cAA6D;AAA9C,IAAA,EAAE,EAAC,cAA2C;AAA3B,IAAA,KAAK,EAAE,QAAA,CAAA,SAAoB;AAAT,IAAA,QAAQ,EAAR;AAAS,GAAtF,E,IAAA,E;;AAAA,I,WAAA,CACE,CAFN,C,CAIE,CAxBP,CAyBG,CA1BN,C","sourcesContent":["//v2 update - added commandStore commands\n\n<template>\n<div class='m-0 p-0 bg-white'>\n   <div class=\"form-group row justify-content-center align-items-center\">\n        <label class=\"col-lg-2 col-sm-6 col-form-label\" for=\"voltage\">Input voltage ({{getVoltageAsString}}V)</label>\n        <div class=\"col-lg-4\" @mousedown=\"setDraggable(false)\" @mouseup=\"setDraggable(true)\">\n            <input type=\"range\" class=\"form-range\" :min=\"-maxV\" :max=\"maxV\" step=\"0.01\" v-model=\"voltage\" list='tickmarks' id=\"voltage\" @change='setVoltage'>\n            <div class='row'>\n                <div class='col-4'><button type='button' class='btn btn-outline-primary btn-sm' @click='incrementVoltage(-0.1)'>-</button></div>\n                <div class='col-4'><label type='label' class='col-form-label'>0.1V</label></div>\n                <div class='col-4'><button type='button' class='btn btn-outline-primary btn-sm' @click='incrementVoltage(+0.1)'>+</button></div>\n            </div>\n            <div class='row'>\n                <div class='col-4'><button type='button' class='btn btn-outline-primary btn-sm' @click='incrementVoltage(-0.01)'>-</button></div>\n                <div class='col-4'><label class='col-form-label'>0.01V</label></div>\n                <div class='col-4'><button type='button' class='btn btn-outline-primary btn-sm' @click='incrementVoltage(+0.01)'>+</button></div>\n            </div>\n        </div>\n\n        <label class=\"col-lg-2 col-sm-6 col-form-label\" for=\"ang_vel\">Motor angular velocity (rad/s)</label>\n        <div class='col-lg-4 col-sm-12' v-if='isAnalogueOutput'>\n            <analogue-output :outputValue=\"getCurrentAngularVelocity\" :minValue=\"0\" :maxValue=\"400\" :intervalValue=\"50\" :minorIntervalValue=\"10\"></analogue-output>\n        </div>\n        <div v-else class='col-lg-4 col-sm-12'>\n            <input type='text' class='form-control' id=\"ang_velocity\" :value='avgAngVel' readonly>\n        </div>\n\n    </div>\n</div>\n\n</template>\n\n<script>\nimport { mapActions, mapGetters } from 'vuex';\nimport AnalogueOutput from \"./AnalogueOutput.vue\";\n\nexport default {\n\n  name: 'DCMotorPanel',\n  props: {\n      dataSocket: WebSocket,\n      maxV: Number,\n  },\n  components: {\n    AnalogueOutput\n  },\n  data () {\n    return {\n        voltage: 0,\n        isAnalogueOutput: true,\n    }\n  },\n  computed:{\n      ...mapGetters([\n          'getCurrentAngularVelocity',\n          'calculateAverageVelocity',\n      ]),\n      getVoltageAsString(){\n          let num = Number(this.voltage);\n          return num.toFixed(2);\n      },\n      avgAngVel(){\n          let average = this.calculateAverageVelocity;\n          return average.toFixed(2);\n      }\n  },\n  methods: {\n      ...mapActions([\n          'setDraggable'\n      ]),\n      setVoltage(){\n          this.$store.dispatch('setVoltage', this.voltage);\n      },\n      incrementVoltage(delta){\n          if(Number(this.voltage) + Number(delta) <= this.maxV){\n              this.voltage = Number(this.voltage) + Number(delta);\n              this.setVoltage();\n          }\n          \n      }\n      \n      \n  }\n}\n</script>\n\n<style scoped>\n\n\n\n</style>"],"sourceRoot":""}]}