#!/bin/sh
mkdir -p ../autogenerated 
cd ../autogenerated

# example: sessionaccess https://relay-access.practable.io spin 32 34 #makes files for spin32, spin33, and spin34

for i in $(seq $3 $4)
do
    num=$(printf "%02d" $i)
	name="${2}${num}"
	echo "${1}/session/${name}-data" > "data.access.${name}"  
	echo "${1}/session/${name}-video" > "video.access.${name}"
done




